{
  "manifest_version": 3,
  "name": "Clear Login Info",
  "version": "1.0",
  "description": "A Chrome extension to clear login information.",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "cookies",
    "storage",
    "activeTab",
    "tabs",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ]
}
